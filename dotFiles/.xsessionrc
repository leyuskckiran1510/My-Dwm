#!/usr/bin/bash
slstatus &
picom --backend glx --vsync &
dunst &
/usr/lib/x86_64-linux-gnu/libexec/kdeconnectd &
~/scripts/bgchange &
pulseaudio &
copyq &
/home/tester/scripts/screenrec &
setxkbmap -option caps:escape &
if [ ! -d "/home/tester/.cache/wal/" ];then
	mkdir -p "/home/tester/.cache/wal/" &&
	echo 'static const char norm_bg[]       = "#222222";
static const char norm_border[]       = "#444444";
static const char norm_fg[]       = "#bbbbbb";
static const char sel_fg[]       = "#eeeeee";
static const char sel_bg[]        = "#005577";
static const char *colors[][3]      = {
	/*               fg         bg         border   */
	[SchemeNorm] = { norm_fg, norm_bg, norm_border },
	[SchemeSel]  = { sel_fg,  sel_bg,   sel_border  },
};' >> "/home/tester/.cache/wal/mine_colors-wal-dwm.h" &
fi

